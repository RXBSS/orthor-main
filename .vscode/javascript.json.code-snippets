	"php-class": {
		"prefix": [
			"class-php"
		],
		"body": [
			"<?php",
			"",
			"class ${1:Klasse} {",
			"",
			"public $$table = \"${2:table}\";",
			"",
			"\tfunction __construct() {",
			"\t\t// Do Something at Construction",
			"\t}",
			"",
			"\tpublic function get($$id) {",
			"\t\t",
			"\t\tglobal $$db;",
			"\t\t$$data = false;",
			"\t\t",
			"\t\t$$query = \"SELECT * FROM `\".$$this->table.\"` WHERE `id` = '\".$$id.\"'\";",
			"\t\t$$result = $$db->query($$query);",
			"\t\tif($$result->num_rows == 1) {",
			"\t\t\t$$data = $$result->fetch_assoc();",
			"\t\t}",
			"",
			"\t\treturn $$data;",
			"\t}",
			"",
			"\t// Neu erstellen",
			"\tpublic function new($$data) {",
			"",
			"\t\tglobal $$db;",
			"\t\t$$success = $$error = $$id = false;",
			"",
			"\t\t// Query",
			"\t\t$$query = \"INSERT INTO `\".$$this->table.\"` SET ",
			"\t\t\t`field1` = '\".$$data['field1'].\"',",
			"\t\t\t`field2` = '\".$$data['field2'].\"'",
			"\t\t\";",
			"\t\t",
			"\t\tif($$db->query($$query)) {",
			"\t\t\t$$id = $$db->insert_id;",
			"\t\t} else {",
			"\t\t\t$$error = $$db->error;",
			"\t\t}",
			"",
			"\t\treturn [",
			"\t\t\t'success' => $$success,",
			"\t\t\t'error' => $$error,",
			"\t\t\t'id' => $$id",
			"\t\t];",
			"\t}",
			"",
			"\tpublic function edit($$id, $$data) {",
			"\t\t",
			"\t\tglobal $$db;",
			"\t\t$$success = $$error = false;",
			"",
			"\t\t$$query = \"UPDATE `\".$$this->table.\"` SET ",
			"\t\t\t`field1` = '\".$$data['field1'].\"',",
			"\t\t\t`field2` = '\".$$data['field2'].\"'",
			"\t\t\tWHERE `id` = '\".$$id.\"';",
			"\t\t \";",
			"",
			"\t\tif($$db->query($$query)) {",
			"\t\t\t$$success = true;",
			"\t\t} else {",
			"\t\t\t$$error = $$db->error;",
			"\t\t}",
			"",
			"\t\treturn [",
			"\t\t\t'success' => $$success,",
			"\t\t\t'error' => $$error",
			"\t\t];",
			"",
			"\t}",
			"",
			"\tpublic function delete($$id) {",
			"\t\t",
			"\t\tglobal $$db;",
			"\t\t$$success = $$error = false;",
			"",
			"\t\t$$query = \"DELETE  `\".$$this->table.\"` WHERE `id` = '\".$$id.\"';\";",
			"\t\t",
			"\t\tif($$db->query($$query)) {",
			"\t\t\t$$success = true;",
			"\t\t} else {",
			"\t\t\t$$error = $$db->error;",
			"\t\t}",
			"",
			"\t\treturn [",
			"\t\t\t'success' => $$success,",
			"\t\t\t'error' => $$error",
			"\t\t];",
			"\t}",
			"}",
			"?>"
		],
		"description": [
			"Standard Aufbau einer PHP Klasse"
		]
