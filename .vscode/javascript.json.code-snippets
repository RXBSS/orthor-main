	"class-php-2": {
		"prefix": [
			"class-php-2",
			"class-php-new"
		],
		"body": [
			"<?php",
			"",
			"class ${1:Klasse} {",
			"",
			"public $$table =  \"${2:table}\";",
			"",
			"\tfunction __construct() {",
			"\t\t// Do Something at Construction",
			"\t}",
			"",
			"\tpublic function get($$id) {",
			"",
			"\t\t$$req = new Request();",
			"",
			"\t\t$$req->get($$this->table, $$id);",
			"",
			"\t\treturn $$req->answer();",
			"\t}",
			"",
			"\t// Neu erstellen",
			"\tpublic function new($$data) {",
			"",
			"\t\t$$req = new Request($$data);",
			"",
			"\t\t$$process = [",
			"\t\t\t['t', 'laufzeit'],",
			"\t\t\t['s', 'status'],",
			"\t\t\t['c', 'vertragsende'],",
			"\t\t\t['dt', 'vertragsbeginn']",
			"\t\t];",
			"",
			"\t\t$$req->insert($$this->table, $$process);",
			"",
			"\t\treturn $$req->answer();",
			"\t}",
			"",
			"\tpublic function edit($$id, $$data) {",
			"",
			"\t\t$$req = new Request($$data);",
			"",
			"\t\t$$process = [",
			"\t\t\t['t', 'laufzeit'],",
			"\t\t\t['s', 'status_id'],",
			"\t\t\t['c', 'vertragsende'],",
			"\t\t\t['dt', 'vertragsbeginn']",
			"\t\t];",
			"",
			"\t\t$$req->update($$this->table, $$process, 'WHERE `id` = '. $$id .'');",
			"",
			"\t\treturn $$req->answer();",
			"",
			"\t}",
			"",
			"\tpublic function delete($$id) {",
			"",
			"\t\t// Request",
			"\t\t$$req = new Request();",
			"",
			"\t\t// Mehrere Löschen",
			"\t\t$$req->deleteMultiple($$this->tableZaehler, $$id);",
			"",
			"\t\t// Rückgabe",
			"\t\treturn $$req->answer();",
			"\t}",
			"}",
			"?>"
		]
	},
